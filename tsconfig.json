{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "paths": {
      "@database": ["database"],
      "@database/*": ["database/*"],
      "@handlers/*": ["handlers/*"],
      "@entities": ["entities"],
      "@utils": ["utils"],
      "@tests/*": [".jest/*"],
      "@seeds/*": [".seeds/*"]
    }
  }
}
