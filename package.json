{
  "name": "slack-bot-for-task-rotation",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "scripts": {
    "start:ngrok": "ngrok http 3000",
    "start:dynamodb": "yarn sls dynamodb start --stage development",
    "start:offline": "yarn serverless offline --httpPort=3000 --stage development --noPrependStageInUrl",
    "deploy:staging": "yarn serverless deploy --stage staging",
    "deploy:production": "yarn serverless deploy --stage production",
    "test": "yarn jest",
    "lint": "yarn eslint .",
    "typecheck": "yarn tsc --noEmit",
    "format": "yarn prettier --check .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.58.0",
    "@aws-sdk/lib-dynamodb": "^3.58.0",
    "@slack/bolt": "^3.9.0",
    "dayjs": "^1.10.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@shelf/jest-dynamodb": "^2.2.3",
    "@types/jest": "^27.4.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-jest": "^27.5.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.6.2",
    "serverless": "^3.2.1",
    "serverless-dotenv-plugin": "^3.12.2",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-offline": "^8.5.0",
    "serverless-plugin-typescript": "^2.1.1",
    "serverless-stage-manager": "^1.0.5",
    "serverless-tscpaths": "^0.0.6",
    "typescript": "^4.5.5"
  }
}
